@model IEnumerable<string>

@{
    ViewBag.Title = "Info";
}

<h2>Wochentage</h2>

<table class="table table-striped table-bordered table-hover">
    <tr>
        <th>Tag</th>
        <th>Länge</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item
        </td>
        <td>
            @item.Length
        </td>
    </tr>
}

</table>
